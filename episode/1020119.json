            "SpeakerIconId": "201"
        },
        {
            "Id": 1020119028,
            "EpisodeMasterId": 1020119,
            "Order": 1,
            "GroupOrder": 27,
            "SpeakerName": "スカウトマン",
            "FontSize": 2,
            "Phrase": "なっ……！？",
            "VoiceFileName": "10201190250",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 1020119029,
            "EpisodeMasterId": 1020119,
            "Order": 1,
            "GroupOrder": 28,
            "SpeakerName": "初魅",
            "FontSize": 2,
            "Phrase": "Edenにすら及ばないような劇団に興味はない。/nさぁ、お引取り願おうか。",
            "VoiceFileName": "10201190260",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 109,
                    "HeadDirectionMasterId": 1,
                    "BodyMotionMasterId": 3,
                    "LipSyncMasterId": 1,
                    "SpineId": 20103,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
