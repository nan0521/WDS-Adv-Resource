            "SpeakerIconId": "201"
        },
        {
            "Id": 1020119030,
            "EpisodeMasterId": 1020119,
            "Order": 1,
            "GroupOrder": 29,
            "SpeakerName": "スカウトマン",
            "FontSize": 2,
            "Phrase": "……いつか、後悔するぞ？",
            "VoiceFileName": "10201190270",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
