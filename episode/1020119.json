                    "slotNumber": 1,
                    "FacialExpressionMasterId": 101,
                    "HeadDirectionMasterId": 1,
                    "BodyMotionMasterId": 24,
                    "LipSyncMasterId": 2,
                    "SpineId": 20103,
                    "CharacterAppearanceType": 0,
