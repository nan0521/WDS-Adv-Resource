        {
            "Id": 1020119032,
            "EpisodeMasterId": 1020119,
            "Order": 1,
            "GroupOrder": 31,
            "SpeakerName": "スカウトマン",
            "FontSize": 2,
            "Phrase": "ここまで傲慢で、我が強いとはな……。/n君のような役者は、こちらとしても願い下げだ。失礼する。",
            "VoiceFileName": "10201190290",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
        },
        {
            "Id": 1020119033,
            "EpisodeMasterId": 1020119,
            "Order": 1,
            "GroupOrder": 32,
            "SpeakerName": "初魅",
            "FontSize": 2,
            "Phrase": "……しまった。どうせ追い払うなら、/nもう少しほめちぎらせておくんだった。",
