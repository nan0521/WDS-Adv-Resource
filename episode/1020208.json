                    "slotNumber": 1,
                    "LipSyncMasterId": 4,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
