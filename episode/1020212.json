            "GroupOrder": 2,
            "SpeakerName": "しぐれ",
            "FontSize": 2,
            "Phrase": "ママ！　おかえりなさい！　/nあれ？　パパも一緒なの？　",
            "VoiceFileName": "10202120010",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 200,
                    "BodyMotionMasterId": 20,
                    "LipSyncMasterId": 2,
